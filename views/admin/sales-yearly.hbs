<div class="content-page">
  <!-- Start content -->
  <div class="content">
    <div class="container-fluid">

      <h4 class="page-title text-center p-2">Weekly Sales Report-{{currentYear}}</h4>
<div class="row">
    <div class="col-12">
        <div class="card m-b-30">
            <div class="card-body">
                <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap"
                    style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                    <thead>
                        <tr>
                            <th>Years</th>
                            {{#each yearlySalesReport}}
                            <th>{{this.year}}</th>
                            {{/each}}
                           
                        </tr>
                    </thead>

                    <tbody>
                        <tr>
                            <td>Revenue</td>
                            {{#each yearlySalesReport}}
                            <td>{{this.grandTotal}}</td>
                            {{/each}}
                        </tr>

                    </tbody>
                </table>

            </div>
        </div>
    </div> <!-- end col -->
</div> <!-- end row -->



    </div>
    <!-- container-fluid -->

  </div>
  <!-- content -->

  <footer class="footer">
    <span class="d-none d-sm-inline-block"> Cozmo Admin Panel </span>
  </footer>

</div>

